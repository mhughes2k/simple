{include file="markNedDialog.tpl"}<!-- <div id="nedHeadings" class="nedHeadings">	<table class="ned">		<tr><th>{$strings.MSG_STAFF_ACTIVITIES}</th></tr>		<tr><th>{$strings.MSG_NPC_ACTIVITIES}</th></tr>		<tr><th>{$strings.MSG_PLAYER_ACTIVITIES}</th></tr>		<tr><th>{$strings.MSG_CRITICAL_EVENTS}</th></tr>	</table></div> --><div id="ned_view">		{if count($events) >0}			<table  border="1" class="ned">			<tr style="background-color:lightgray" title="{$strings.MSG_STAFF_ACTIVITIES}">			<th>{$strings.MSG_STAFF_ACTIVITIES}</th>			{foreach from=$events item=event name=sa}        {if $event.itemtype==4}					<td style="overflow:auto;background-color:#{$event.color|default:'ffffff'}">					              {if $event.state==""}                <A class="tagned" eventid="{$event.projecttemplateeventid}" title="Tag '{$event.name}'"><strong>{$event.name}</strong></a><br/>                             {else}              	<A class="tagned" eventid="{$event.projecttemplateeventid}" eventstate="{$event.state}" title="Change '{$event.name}' tag"><strong>{$event.name}</strong></a>              	[{$event.state}]              {/if}					</td>        {elseif $event.itemtype==3}        <!-- Release Doc//-->        		<td rowspan="4" style="overflow:auto;background-color:#{$event.color|default:'ffffff'}">				                {if $ned_EnableDocuments}						<a href="index.php?option=projectTemplateAdmin&cmd=editDocument&docUid={$event.nexteventid}"><strong>{$event.name}</strong></a>						{else}              <strong><a target="_blank" href="index.php?option=download&pid={$pid}&docType=doc_templ&docuid={$event.nexteventid}">{$event.name}</a></strong>						{/if}						{if $event.state!=''}							<br>[{$event.state}]						{/if}						{if $ned_EnableTriggers & $event.hasChildren}												<!-- <A href="?option=projectadmin&cmd=triggerevent&id={$event.projecttemplateeventid}&pid={$pid}" title="{$strings.MSG_TRIGGER_LABEL}'{$event.name}'"><img src="{$config.ned_icon_trigger}"></a> -->													<br><a class="triggerned" eventid="{$event.projecttemplateeventid}" projectid="{$pid}" title="{$strings.MSG_TRIGGER_LABEL}{$event.name}"><img src="{$config.ned_icon_trigger}"></a>						{/if}				{elseif $event.itemtype==100}						  <td rowspan="4" style="background-color:black;color:white">          {$event.name}          </td>        {else}        <td></td>        {/if}        			{/foreach}			</tr>			<tr title="{$strings.MSG_NPC_ACTIVITIES}">			<th>{$strings.MSG_NPC_ACTIVITIES}</th>			{foreach from=$events item=event name=npc}        {if $event.itemtype==2}          <td class="ned" style="background-color:#{$event.color|default:'ffffff'};overflow:auto">             						&lt;{$event.performerrole}&gt;              {if $event.state==''}  							{if $ned_EnableStates}  								<A class="tagned" eventid="{$event.projecttemplateeventid}"  title="{$strings.MSG_NED_TAG_LABEL}'{$event.name}'"><strong>{$event.name}</strong></a>  							{else}  								{$event.name}  							{/if}  						{else}  							{if $ned_EnableStates}  								<A  class="tagned" eventid="{$event.projecttemplateeventid}" eventstate="{$event.state}" title="{$strings.MSG_CHANGE_TAG_LABEL}'{$event.name}' tag"><strong>{$event.name}</strong></a>  								<br/>[{$event.state}]  							{else}  								{$event.name}  							{/if}			  						{/if}  						{if $ned_EnableTriggers and $event.hasChildren}  						  							<br><a class="triggerned" eventid="{$event.projecttemplateeventid}" projectid="{$pid}" title="{$strings.MSG_TRIGGER_LABEL}{$event.name}"><img src="{$config.ned_icon_trigger}"></a>  							<!--<A href="?option=projectadmin&cmd=triggerevent&id={$event.projecttemplateeventid}&pid={$pid}" title="{$strings.MSG_TRIGGER_LABEL}'{$event.name}'">T<img src="{$config.ned_icon_trigger}"></a>-->  						{/if}            </td>        {elseif $event.itemtype==3}				{elseif $event.itemtype==100}        {else}          <td></td>        {/if}			{/foreach}			</tr>			<tr style="background-color:lightgray" title="{$strings.MSG_PLAYER_ACTIVITIES}">			<th>{$strings.MSG_PLAYER_ACTIVITIES}</th>			{foreach from=$events item=event name=pc}			  {if $event.itemtype==1}          {if $event.performerrole=="CHAR_PLAYER"}            <td class="ned" style="background-color:#{$event.color|default:'ffffff'};overflow:auto">						{if $event.state==''}							{if $ned_EnableStates}							<A class="tagned" eventid="{$event.projecttemplateeventid}"  title="{$strings.MSG_NED_TAG_LABEL}'{$event.name}'"><strong>{$event.name}</strong></a>							{else}								<strong>{$event.name}</strong>							{/if}						{else}							{if $ned_EnableStates}								<A class="tagned" eventid="{$event.projecttemplateeventid}" eventstate="{$event.state}"  title="{$strings.MSG_CHANGE_TAG_LABEL}'{$event.name}' note"><strong>{$event.name}</strong></a>								<br/>[{$event.state}]							{else}								<strong>{$event.name}</strong>							{/if}						{/if}					{else}					<td />					{/if}				{elseif $event.itemtype==100}        {elseif $event.itemtype==3}	      {else}        <td></td>        {/if}			{/foreach}			</tr>			<tr title="{$strings.MSG_CRITICAL_EVENTS}">			<th>{$strings.MSG_CRITICAL_EVENTS}</th>			{foreach from=$events item=event name=ce}			  {if $event.itemtype==0}          <td class="ned {if $event.itemtype==0}criticalEvent{/if}" style="overflow:auto;background-color:#{$event.color|default:'ffffff'}">            {if $event.state==''}						  {if $ned_EnableTriggers}								<a class="triggerned" eventid="{$event.projecttemplateeventid}" projectid="{$pid}" title="{$strings.MSG_TRIGGER_LABEL}{$event.name}"><strong>{$event.name}</strong><img src="{$config.ned_icon_trigger}"></a>								<!-- <A href="?option=projectadmin&cmd=triggerevent&id={$event.projecttemplateeventid}&pid={$pid}" title="{$strings.MSG_TRIGGER_LABEL}{$event.name}"><strong>{$event.name}</strong><img src="{$config.ned_icon_trigger}"></a>-->							{else}								<strong>{$event.name}</strong>							{/if}						{else}							<strong>{$event.name}</strong><br/>[{$event.state}]<A class="triggerned" eventid="{$event.projecttemplateeventid}" projectid="{$pid}" title="{$strings.MSG_RETRIGGER_LABEL}'{$event.name}'"><img src="{$config.ned_icon_trigger}"></a>						{/if}					</td>				{elseif $event.itemtype==100}        {elseif $event.itemtype==3}	      {else}          <td></td>				{/if}			{/foreach}			</tr>			</table>		{else}			{$strings.MSG_NO_PROJECT_STRUCTURE_FOUND}		{/if}</div>